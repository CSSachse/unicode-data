{
  "compilerOptions": {
    "target": "es2018",
    "lib": [
      "dom",
      "es6"
    ],
    "outDir": "dist",
    "module": "commonjs",
    "jsx": "react",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true,
    "pretty": true,
    "skipLibCheck": true,
    "newLine": "LF",
    "sourceMap": true,
    "inlineSources": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true
  }
}